[run]
branch = True
source =
    esimport
omit =
    *tests*
    esimport/__main__.py
    setup.py

[report]
show_missing = True
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    logger*
    traceback*
